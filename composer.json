{
  "name": "iliad/api-core",
  "description": "description",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "deckersilyas@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ody/container": "dev-master",
    "ody/influxdb": "dev-master",
    "ody/logger": "dev-master",
    "ody/server": "dev-master",
    "ody/swoole": "dev-master",
    "ody/database": "dev-master",
    "ody/foundation": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Ody\\Support\\": "src/Support/",
      "Ody\\Auth\\": "src/Auth/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../foundation"
    }
  ]
}

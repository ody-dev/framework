{
  "name": "iliad/api-core",
  "description": "description",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "deckersilyas@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ody/server": ">=0.1.0",
    "ody/database": "dev-master",
    "ody/foundation": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Ody\\Auth\\": "src/Auth/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }

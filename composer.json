{
  "name": "iliad/api-core",
  "description": "description",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "deckersilyas@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ody/container": "dev-master",
    "ody/influxdb": "@dev",
    "ody/logger": "dev-master",
    "ody/server": "@dev",
    "ody/swoole": "dev-master",
    "ody/database": "@dev",
    "ext-swoole": ">=6.0.0",
    "nikic/fast-route": "^1.3",
    "monolog/monolog": "^2.0",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0.2",
    "psr/http-server-handler": "^1.0.2",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^7.2",
    "symfony/process": "^7.2",
    "swoole/ide-helper": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php",
      "src/Foundation/Http/functions/normalize_uploaded_files.php",
      "src/Foundation/Http/functions/create_uploaded_file.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Ody\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../ody-server"
    },
    {
      "type": "path",
      "url": "../InfluxDB"
    },
    {
      "type": "path",
      "url": "../ody-database"
    }
  ]
}
